  }\n                                                                data={\n                                                                    this.state\n                                                                        .productDetails !=\n                                                                        null &&\n                                                                    this.state\n                                                                        .productDetails\n                                                                }\n                                                                onChange={\n                                                                    this\n                                                                        .handleProductDetailsChange\n                                                                }\n                                                            />\n                                                        </div>\n                                                    </div>\n                                                    {this.state.categories && (\n                                                        <div className=\"col-sm-6\">\n                                                            {/* select */}\n                                                            <div className=\"form-group\">\n                                                                <label>\n                                                                    Loại sản\n                                                                    phẩm\n                                                                </label>\n\n                                                                <select\n                                                                    className=\"form-control\"\n                                                                    value={\n                                              