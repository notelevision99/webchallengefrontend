                           </div>\n                                                    </div>\n                                                    <div className=\"col-sm-6\">\n                                                        {/* textarea */}\n                                                        <div className=\"form-group\">\n                                                            <label>\n                                                                Chi tiết sản\n                                                                phẩm\n                                                            </label>\n                                                            <CKEditor\n                                                                editor={\n                                                                    ClassicEditor\n                                                                }\n                                                                data={\n                                                                    this.state\n                                                                        .productDetails\n                                                                }\n                                                                onChange={\n                                                                    this\n                                                                        .handleProductDetailsChange\n                                                                }\n                                                            />\n                                                        </div>\n                                                    </div>\n                                                    {/* Load ListCategories */}\n                                                    {this.state.categories ? (\n                                                        <div className=\"col-sm-6\">\n                  