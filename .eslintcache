lassName=\"form-control\"\n                                                                placeholder=\"Enter ...\"\n                                                            />\n                                                        </div>\n                                                    </div>\n                                                </div>\n                                                <div className=\"row\">\n                                                    <div className=\"col-sm-6\">\n                                                        {/* textarea */}\n                                                        <div className=\"form-group\">\n                                                            <label>\n                                                                Mô tả sản phẩm\n                                                            </label>\n                                                            <CKEditor\n                                                                editor={\n                                                                    ClassicEditor\n                                                                }\n                                                                data={\n                                                                    this.state\n                                                                        .desription\n                                                                }\n                                                                onChange={\n                                                                    this\n                                                                        .handleDescriptionChange\n                                                                }\n                                                            />\n                                                        </div>\n                                                    </div>\n